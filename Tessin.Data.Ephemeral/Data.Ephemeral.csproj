<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net461</TargetFramework>
    <Authors>Tessin Nordic AB</Authors>
    <AssemblyName>Tessin.Data.Ephemeral</AssemblyName>
    <RootNamespace>Tessin.Data.Ephemeral</RootNamespace>
    <PackageId>Tessin.Data.Ephemeral</PackageId>
